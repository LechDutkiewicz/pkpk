{
  "entry": {
    "main": [
      "./scripts/main.js",
      "./styles/main.scss"
    ],
    "customizer": [
      "./scripts/customizer.js"
    ],
    "admin": [
      "./scripts/admin.js",
      "./styles/admin.scss"
    ]
  },
  "watch": [
    "templates/**/*.php",
    "src/**/*.php",
    "assets/scripts/**/*.js",
    "assets/styles/**/*.scss"
  ],
  "publicPath": "/pkpk/wp-content/themes/pkpk",
  "devUrl": "http://localhost/pkpk",
  "proxyUrl": "http://localhost:3000",
  "cacheBusting": "[name]_[hash:8]",
  "browsers": [
    "last 2 versions",
    "android 4",
    "opera 12"
  ]
}
